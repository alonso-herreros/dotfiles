    "group/status": {
        "orientation": "inherit",
        "modules": [
            // "group/backlight",
            "group/audio",
            "group/bluetooth-group",
            "group/network-group"
        ]
    },

    // Audio
    "group/audio": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200
        },
        "modules": [
            "pulseaudio",
            "pulseaudio#pa2"
        ]
    },

    "pulseaudio": {
        "format": "{icon}",
        "format-muted": "",
        "format-icons": {
            "headphone": " ",
            "hands-free": " ",
            "headset": " ",
            "phone": "",
            "portable": "",
            "car": "",
            "hdmi": "",
            "default": [" ", " ", " "]
        },
        "tooltip-format": "{desc}: {volume}%",
        "on-click": "pamixer -t",
        "on-click-right": "hyprctl dispatch exec [floating] pavucontrol",
        "on-click-middle": "pactl play-sample bell",
        "reverse-scrolling": true,
    },
    "pulseaudio#pa2": {
        "format": "{volume}%",
        "tooltip-format": "{desc}: {volume}%",
        "on-click": "pamixer -t",
        "on-click-right": "hyprctl dispatch exec [floating] pavucontrol",
        "on-click-middle": "pactl play-sample bell",
        "reverse-scrolling": true,
    },

    "group/bluetooth-group": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200
        },
        "modules": [
            "bluetooth",
            "bluetooth#bt2"
        ]
    },
    "bluetooth": {
        "format": "",
        "format-connected": "··",
        "tooltip-format": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "• {device_alias}",
        "on-click": "~/.scripts/bluetooth-toggle",
        // "on-click-right": "bluetoothctl"
    },
    "bluetooth#bt2": {
        "format": "{num_connections}",
        "tooltip-format": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "• {device_alias}",
        "on-click": "~/.scripts/bluetooth-toggle",
        // "on-click-right": "bluetoothctl"
    },


    "group/network-group": {
        "orientation": "inherit",
        "drawer": {
            "transition-left-to-right": false,
            "transition-duration": 200
        },
        "modules": [
            "network",
            "network#nw2"
        ]
    },
    "network": {
        "interval": 10,
        "format-wifi": " ",
        "format-ethernet": " ",
        "format-linked": "{ifname} (No IP) ",
        "format-alt": "{bandwidthDownBytes}  {bandwidthUpBytes}",
        "tooltip-format-wifi": "  {} ({frequency} GHz) {signalStrength}% | {signaldBm} dBm\n{ifname}: {ipaddr}/{cidr} via {gwaddr}",
        "tooltip-format-ethernet": "  {ifname}: {ipaddr}/{cidr} via {gwaddr}"
    },
    "network#nw2": {
        "interval": 10,
        "format-wifi": "{}"
    },

    "group/session": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 200,
            "transition-left-to-right": false
        },
        "modules": [
            "user",
            // "user#info"
            "custom/restart",
            "custom/hibernate",
            "custom/poweroff",
            "custom/lock",
            "custom/exit",
        ]
    },
    
    "user": {
        "interval": 60,
        "format": "",
        "tooltip-format": "{user}",
        "icon": true,
        "height": 20,
        "width": 20,
        "open-on-click": false,
        "on-click-right": "hyprlock"
    },
    // "user#info": {
    //     "interval": 60,
    //     "format": " Uptime: {work_d} d {work_H}:{work_M}"
    // },

    "custom/exit": {
        "format": " ",
        "tooltip-format": "Exit session",
        "on-click": "hyprctl dispatch exit"
    },
    "custom/lock": {
        "format": " ",
        "tooltip-format": "Lock screen",
        "on-click": "hyprlock"
    },
    "custom/poweroff": {
        "format": " ",
        "tooltip-format": "Power Off",
        "on-click": "systemctl poweroff",
        "on-click-right": "systemctl poweroff -i"
    },
    "custom/restart": {
        "format": " ",
        "tooltip-format": "Restart",
        "on-click": "systemctl reboot -i",
        "on-click-right": "systemctl reboot",
        "on-click-middle": "systemctl soft-reboot -i"
    },
    "custom/hibernate": {
        "format": " ",
        "tooltip-format": "Hibernate",
        "on-click": "hyprlock & sleep 0.5; systemctl hibernate",
        "on-click-right": "hyprlock & sleep 0.5; systemctl hibernate -i"
    },

}
